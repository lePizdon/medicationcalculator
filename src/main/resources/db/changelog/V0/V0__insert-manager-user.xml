<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.15.xsd">

    <changeSet id="V0-4.0" author="lePizdon">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT
                    SUM(
                        (SELECT COUNT(*) FROM security.users) +
                        (SELECT COUNT(*) FROM security.authority) +
                        (SELECT COUNT(*) FROM security.users_authority)
                    )
            </sqlCheck>
        </preConditions>
        <sqlFile path="pre-create-manager.sql" relativeToChangelogFile="true"/>
    </changeSet>

</databaseChangeLog>